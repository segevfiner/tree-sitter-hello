{
  "name": "tree-sitter-hello",
  "version": "0.0.1",
  "description": "Hello grammar for tree-sitter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/segevfiner/tree-sitter-hello.git"
  },
  "license": "MIT",
  "main": "bindings/node",
  "files": [
    "grammar.js",
    "binding.gyp",
    "types/dsl.d.ts",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "hello"
  ],
  "dependencies": {
    "node-addon-api": "^7.1.0",
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "prebuild": "^13.0.0",
    "tree-sitter-cli": "^0.21.0"
  },
  "binary": {
    "napi_versions": [
      8
    ]
  },
  "scripts": {
    "install": "prebuild-install -r napi || node-gyp rebuild",
    "build": "tree-sitter generate --no-bindings",
    "build-wasm": "tree-sitter build-wasm",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.hello",
      "injection-regex": "^hello$"
    }
  ]
}
